{
  "name": "Color Magnifier",
  "description": "Color Magnifier",
  "version": "0.0.1",
  "author": "Yong Su",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Color Magnifier"
  },
  "options_page": "",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "fonts/*"
  ]
}
